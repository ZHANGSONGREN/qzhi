.cu-title.center-title {
	text-align: center;
}
.cu-title.center-title .text-xl {
	position: relative;
	display: inline-block;
	padding: 0 30rpx;
}
.cu-title.center-title .text-xl::after,
.cu-title.center-title .text-xl::before {
	content: "";
	display: block;
	position: absolute;
	width: 40%;
	height: 1rpx;
	background-color: #333;
	left: -40%;
	right: auto;
	top: 0;
	bottom: 0;
	margin: auto;
}
.cu-title.center-title .text-xl::before {
	left: auto;
	right: -40%;
}

